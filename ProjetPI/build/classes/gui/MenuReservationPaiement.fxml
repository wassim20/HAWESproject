<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MenuReservationPaiementController">
   <children>
      <Label alignment="CENTER" layoutX="180.0" layoutY="36.0" prefHeight="69.0" prefWidth="441.0" text="Menu Réservation et Paiement">
         <font>
            <Font name="Corbel" size="31.0" />
         </font>
      </Label>
      <Button fx:id="btnaffr" layoutX="494.0" layoutY="143.0" mnemonicParsing="false" onAction="#afficherReservations" prefHeight="31.0" prefWidth="254.0" text="Afficher tous les Réservations" />
      <Button fx:id="btntrtr" layoutX="121.0" layoutY="211.0" mnemonicParsing="false" onAction="#traiterReservation" prefHeight="31.0" prefWidth="254.0" text="Traiter les Réservations" />
      <Button fx:id="btnaffp" layoutX="121.0" layoutY="143.0" mnemonicParsing="false" onAction="#afficherPaiements" prefHeight="31.0" prefWidth="254.0" text="Afficher tous les Paiements" />
      <Button fx:id="btnrect" layoutX="121.0" layoutY="275.0" mnemonicParsing="false" onAction="#recupererTicket" prefHeight="31.0" prefWidth="254.0" text="Récupérer une Ticket" />
      <Button fx:id="btnajr" layoutX="494.0" layoutY="341.0" mnemonicParsing="false" onAction="#ajouterReservation" prefHeight="31.0" prefWidth="254.0" text="Effectuer une Réservation" />
      <Button fx:id="btnmdr" layoutX="494.0" layoutY="211.0" mnemonicParsing="false" onAction="#modifierReservation" prefHeight="31.0" prefWidth="254.0" text="Modifier une Réservation" />
      <Button fx:id="btnsuppr" layoutX="494.0" layoutY="275.0" mnemonicParsing="false" onAction="#supprimerReservation" prefHeight="31.0" prefWidth="254.0" text="Annuler une Réservation" />
      <Button fx:id="btnstat" layoutX="121.0" layoutY="341.0" mnemonicParsing="false" onAction="#displayStatistics" prefHeight="31.0" prefWidth="254.0" text="Statistiques" />
   </children>
</AnchorPane>
